  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-cookie": "^1.0.0",
    "@types/mongodb": "^3.5.25",
    "@types/mongoose": "^5.10.3",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "http-proxy-middleware": "^1.0.5",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "koa-cookie": "^1.0.0",
    "koa-logger": "^3.2.1",
    "koa-proxy-middleware": "^2.0.2",
    "koa-router": "^9.1.0",
    "koa-static": "^5.0.0",
    "mikro-orm": "^3.6.15",
    "mongodb": "^3.6.0",
    "mongoose": "^5.9.27",
    "mongoose-sequence": "^5.2.2",
